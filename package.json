{
  "name": "kindred",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --host lan",
    "android": "expo start --android --host lan",
    "ios": "expo start --ios --host lan",
    "web": "expo start --web",
    "dev:client": "expo start --dev-client --host lan",
